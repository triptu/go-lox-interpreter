class Breakfast {
  serve() {
    return "poha";
  }

  serveDrink() {
    return this.drink;
  }
}

print(Breakfast);
var breakfast = Breakfast();
print(breakfast);

breakfast.drink = "milk";
print("drink in breakfast = " + breakfast.drink); 
print(breakfast.serve());
print("drink for the day - " + breakfast.serveDrink());
